{
  "name": "mongoui",
  "version": "4.2.1",
  "engines": {
    "node": ">= v6.x"
  },
  "bin": "./index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/azat-co/mongoui.git"
  },
  "scripts": {
    "start": "NODE_ENV=production npm run clean && npm run build && node_modules/.bin/concurrently 'node node_modules/.bin/nodemon index.js'",
    "start-dev": "NODE_ENV=dev node_modules/.bin/concurrently 'npm run dev' 'npm run api-server' 'npm run dev-server' 'node_modules/.bin/opn http://localhost:3000'",
    "test": "mocha test",
    "clean": "rm -f ./public/js/bundle.js",
    "build": "node node_modules/.bin/webpack --config webpack.start.config.js",
    "dev": "npm run clean && node node_modules/webpack-dev-server/bin/webpack-dev-server.js --content-base public --hot",
    "dev-server": "node dev-server.js",
    "api-server": "node node_modules/.bin/nodemon index.js",
    "preversion": "npm run build"
  },
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "convict": "^2.0.0",
    "cors": "^2.7.1",
    "deep-equal": "^1.0.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-handlebars": "^3.0.0",
    "getconfig": "^3.0.0",
    "mongodb": "^2.2.30",
    "mongoskin": "2.0.3",
    "neo-async": "1.8.0",
    "opn-cli": "3.1.0",
    "react": "^15.0.1",
    "react-bootstrap": "0.30",
    "react-copy-to-clipboard": "^4.1.0",
    "react-dom": "^15.0.1",
    "react-highlight": "^0.8.0",
    "react-redux": "4.4.1",
    "react-router": "^2.2.2",
    "redux": "3.3.1",
    "require-dir": "^0.3.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.23.1",
    "expect": "1.16.0",
    "extended-define-webpack-plugin": "^0.1.2",
    "json-loader": "^0.5.4",
    "mocha": "2.4.5",
    "nodemon": "^1.9.1",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "3.1.1",
    "request": "^2.69.0",
    "style-loader": "^0.13.1",
    "supertest": "^1.2.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "license": "SEE IN LICENSE.md"
}
